{
  "name": "constance-mw",
  "version": "3.0.0",
  "private": true,
  "description": "",
  "main": "src/app.ts",
  "type": "module",
  "scripts": {
    "build": "tsup --dts --sourcemap --format esm",
    "build-tsc": "npx tsc",
    "start": "node dist/app.js",
    "dev3": "npx tsx watch --inspect=9229 src/app.ts",
    "dev2": "tsup --dts --sourcemap --format esm,cjs --watch --onSuccess \"npx tsx watch --inspect=9229 src/app.ts\"",
    "dev4": "tsup --dts --sourcemap --format esm,cjs --watch --onSuccess \"node --watch --inspect dist/app.js\"",
    "dev": "tsup --dts --sourcemap --format esm --watch --onSuccess \"node dist/app.js -- --inspect\"",
    "check-updates": "ncu"
  },
  "tsup": {
    "entry": [
      "src/app.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "keywords": [],
  "author": "Ndubuisi Emenogu <ndubuisi.emenogu@intel.com>",
  "dependencies": {
    "adm-zip": "^0.5.16",
    "axios": "^1.8.4",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "fast-equals": "^5.2.2",
    "fast-sort": "^3.4.1",
    "fs-extra": "^11.3.0",
    "hash-sum": "^2.0.0",
    "helmet": "^8.1.0",
    "jsondiffpatch": "^0.7.3",
    "mongodb": "^6.15.0",
    "multer": "^1.4.5-lts.2",
    "rfdc": "^1.4.1",
    "ts-simple-nameof": "^1.3.1",
    "xml-formatter": "^3.6.5"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/fs-extra": "^11.0.4",
    "@types/hash-sum": "^1.0.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.14.0",
    "http-request-mock": "^2.0.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  }
}
